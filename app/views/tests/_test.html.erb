<tr>
  <td><%= link_to test.title, start_test_path(test), class: 'text-primary' %></td>
  <td><%= test.level %></td>
  <td><%= test.category.name %></td>
  <td><%= test.author.name %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.timer.positive? ? test.timer : t('.unlimited') %></td>
  <td> <%= link_to t('start'), start_test_path(test), class: 'btn btn-sm btn-success' %></td>
</tr>
